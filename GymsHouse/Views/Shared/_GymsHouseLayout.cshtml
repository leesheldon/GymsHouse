    <script type="text/javascript" src="~/lesnarStyle/assets/revolution/js/extensions/revolution.extension.slideanims.min.js"></script>
    <script type="text/javascript" src="~/lesnarStyle/assets/revolution/js/extensions/revolution.extension.video.min.js"></script>

    <!-- Owl Carosel JS Area -->
    <script src="~/lesnarStyle/assets/js/owl.carousel.js"></script>
    <!-- CSS Menu JS Area -->
    <script src="~/lesnarStyle/assets/js/css-menu.js"></script>
    <!-- Shuffle Plugin JS Area -->
    <script src="~/lesnarStyle/assets/js/jquery.shuffle.min.js"></script>
    <!-- Comming Soon Countdown JS Area -->
    <script src="~/lesnarStyle/assets/js/jquery.countdown.min.js"></script>
    <!-- Counter Section JS Area -->
    <script src="~/lesnarStyle/assets/js/jquery.counterup.min.js"></script>
    <script src="~/lesnarStyle/assets/js/counter.js"></script>
    <script src="~/lesnarStyle/assets/js/waypoints.min.js"></script>

    <!-- mix it js -->
    <script src="~/lesnarStyle/assets/js/jquery.mixitup.min.js"></script>
    <!-- Lesnar Custom JS Area -->
    <script src="~/lesnarStyle/assets/js/custom.js"></script>


    <!-- ============= Stamina ============= -->
    <!-- Waypoints -->
    <script src="~/staminaStyle/js/jquery.waypoints.min.js"></script>
    <!-- Stellar Parallax -->
    <script src="~/staminaStyle/js/jquery.stellar.min.js"></script>
    <!-- Main -->
    <script src="~/staminaStyle/js/main.js"></script>
    <!-- jQuery Easing -->
    <script src="~/staminaStyle/js/jquery.easing.1.3.js"></script>

    @RenderSection("Scripts", false)
</body>
</html>
